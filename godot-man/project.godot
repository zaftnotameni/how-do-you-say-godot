; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="godot-man"
run/main_scene="res://godot/game/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="user-dir"
config/features=PackedStringArray("4.0", "Mobile")
run/max_fps=30
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

XX="*res://godot/lib/global_definitions.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1920
window/size/window_height_override=1080
window/energy_saving/keep_screen_on=false
window/vsync/vsync_mode=2
window/stretch/mode="canvas_items"

[editor]

naming/node_name_num_separator=3
naming/node_name_casing=2

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg")

